     <link href="{{ evalPhp('Layout::path') }}bootstrap/css/bootstrap-toggle.min.css" rel="stylesheet">
     <script src="{{ evalPhp('Layout::path') }}bootstrap/js/bootstrap-toggle.min.js"></script>

     
