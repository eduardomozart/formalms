<!DOCTYPE html>
<html lang="{{ evalPhp('Layout::lang_code') }}">

<head>
     {% block compatibility %}
    		<meta http-equiv="X-UA-Compatible" content="IE=edge">
     {% endblock %}
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{{ layout_title }}</title>
    <!-- zone  -->
    {{ layout_zone_meta}}
    <!-- meta  -->
    {{ layout_meta | raw }}
    <link rel="shortcut icon" href="{{layout_path}}images/favicon.png" type="image/png"/>
    <link rel="shortcut icon" href="{{layout_path}}images/favicon.ico"/>
    


    <!-- reset and font stylesheet -->
    {{ resetter|raw }}
    
    <!-- JQuery, Bootstrap  -->
    {% block jquery_core %}
    {{ jqueryLib|raw }}    
    {% endblock %}

    {% block bootstrap %}
    {{ boostrap|raw }}    
    {% endblock %}
    
    {% block jquery_addons %}
    <!-- addons: js -->    
    {{ jqueryAddons|raw }}    
    {% endblock %}
    
    
    {% block css_addons %}
    <!-- addons: css -->    
    {{ cssAddons|raw }}    
    {% endblock %}


    <!-- specific stylesheet -->    
    <link rel="stylesheet" type="text/css" href="{{ layout_path }}style/base.css"/>    
    <link rel="stylesheet" type="text/css" href="{{ layout_path }}style/responsive.css"/>
    <link rel="stylesheet" type="text/css" href="{{ layout_path }}style/font-awesome/css/font-awesome.min.css" />



    <!-- to be eliminated -->    
    {{ yuiLib}}
    <!-- printer stylesheet-->
    <link rel="stylesheet" type="text/css" href="{{ layout_path }}style/print.css" media="print"/>
    {{ accessibility|raw }}
    <!-- Page Head area -->
    {{ layout_zone_page_head|raw }}
    {{ layout_rtl|raw }}
    {% block pagehead %}
    {% endblock %}
    
    
    <!-- Custom css area -->    
    {% block stylesheet %}
    {% endblock %}

    <!-- Restyling -->
    <link rel="stylesheet" type="text/css" href="{{ layout_path }}../../../web/static/main.css">
    <script type="text/javascript" href="{{ layout_path }}../../../web/static/main.js"></script>

    <!-- Custom Javascripts area -->

    <script type="text/javascript" src="{{ layout_path }}script/tabnav.js"></script>

  {% block javascript %}
    {% endblock %}
    


    
</head>
<body class="yui-skin-docebo yui-skin-sam">
    <div class="container-fluid">      
    {% block content %}
    {% endblock %}
        {{ layout_analytics|raw }}
    </div>
                          
</body>
</html>