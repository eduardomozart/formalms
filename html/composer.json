{
    "name": "formalms/formalms",
    "require": {
        "php": ">=7.0",
        "symfony/event-dispatcher": "^3.3",
        "twig/twig": "~1.34.3",
        "symfony/var-dumper": "~2.8.17",
        "lusitanian/oauth": "~0.8.10",
        "phpmailer/phpmailer": "~5.2.23",
        "ezyang/htmlpurifier": "~4.10.0"
    },
    "require-dev": {
        "phing/phing": "~2.13.0",
        "phpunit/phpunit": "~4.1.6",
        "simpletest/simpletest": "~1.1"
    },
    "authors": [
        {
            "name": "forma.lms project",
            "email": "info@formalms.org"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
