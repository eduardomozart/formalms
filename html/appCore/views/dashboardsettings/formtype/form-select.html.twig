<div class="dashboard__select">
    <label for="{{ block.postData.block }}-{{ index }}-file">{{ translate('_select_'~ form.class,'dashboardsetting') }}</label>
    <select class="form-control" data-block="{{ block.postData.block }}-{{ index }}">

        {% for key,value in form.values %}

            <option value="{{ block.postData.block }}_{{ index }}_{{ key }}"{% if block.type == type %} selected{% endif %}>{{ value }}</option>

        {% endfor %}
    </select>
</div>