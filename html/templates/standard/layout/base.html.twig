<!DOCTYPE html>
<html lang="{{ evalPhp('Layout::lang_code') }}">

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--Fix funzionamento scorm su IE9-->
    <!--[if lt IE 9] -->
    <meta http-equiv="x-ua-compatible" content="IE=9"/>
    <![endif] -->     
    <!--END -->

    
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <title>{{ layout_title }}</title>
    <!-- zone  -->
    {{ layout_zone_meta}}
    <!-- meta  -->
    {{ layout_meta | raw }}
    <link rel="shortcut icon" href="{{layout_path}}images/favicon.png" type="image/png"/>
    <link rel="shortcut icon" href="{{layout_path}}images/favicon.ico"/>
    <!-- reset and font stylesheet -->
    {{ resetter|raw }}
    
    <!-- JQuery, Bootstrap  -->
    {% block jquery_core %}
    {{ jqueryLib|raw }}    
    {% endblock %}

    {% block bootstrap %}
    {{ boostrap|raw }}    
    {% endblock %}
    
    {% block jquery_addons %}
    <!-- addons: js -->    
    {{ jqueryAddons|raw }}    
    {% endblock %}
    
    
    {% block css_addons %}
    <!-- addons: css -->    
    {{ cssAddons|raw }}    
    {% endblock %}


    <!-- specific stylesheet -->    
    <link rel="stylesheet" type="text/css" href="{{ layout_path }}style/base.css"/>    
    <link rel="stylesheet" type="text/css" href="{{ layout_path }}style/responsive.css"/>

    <!-- to be eliminated -->    
    {{ yuiLib}}
    <!-- printer stylesheet-->
    <link rel="stylesheet" type="text/css" href="{{ layout_path }}style/print.css" media="print"/>
    {{ accessibility|raw }}
    <!-- Page Head area -->
    {{ layout_zone_page_head|raw }}
    {{ layout_rtl|raw }}
    {% block pagehead %}
    {% endblock %}
    
    
    <!-- Custom css area -->    
    {% block stylesheet %}
    {% endblock %}

    <!-- Custom Javascripts area -->
    {% block javascript %}
    {% endblock %}
    
    

    
</head>
<body class="yui-skin-docebo yui-skin-sam">
    <div class="container-fluid">      
    {% block content %}
    {% endblock %}
        {{ layout_analytics|raw }}
    </div>
                          
</body>
</html>